<% @messages.each do |message| %>


<div class="card">
  <div class="card-block">
    <div class="row">
      <div class="col-md-11">
        <p class="card-text">
          <span class="text-muted">
            <h1><%= message.title %></h1>
            <%= message.user.first_name %> at <%= message.timestamp %> says</span><br>
            <%= message.body %>
          </p>
        </div>
      </div>
    </div>
  </div>


  <% end %>

