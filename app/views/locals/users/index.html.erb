<% @users.each do |user| %>
<h1> <%= link_to user.first_name, locals_user_path(user) %> </h1>
<h1> <%= user.last_name %> </h1>
<h2><%= user.dedication %></h2>
<h2><%= user.stay %></h2>
<h2> <%= user.age %> </h2>
<h3> <%= user.email %> </h3>
<h3> <%= user.typeofmusic %></h3>
<h3> <%= user.description %> </h3>
<% end %>
