
<div
id="map"
style="width: 100%;
height: 400px;"
data-user_markers="<%= @user_markers.to_json %>"
data-markers="<%= @markers.to_json %>"
></div>


<div class="container">
<% @users.each do |user| %>
<figure class="snip1208">
  <img src="https://www.top10berlin.de/sites/top10berlin.de/files/location/mainimages/2015/10/08/bar-ansicht_dpa.jpg" alt="sample66"/>
  <div class="date"><span class="day"><%= @event.time.strftime("%H:%M") %></span><span class="month">Today</span></div><i class="ion-beer"></i>
  <figcaption>
    <p><%= link_to user.first_name, locals_event_user_path(@event, user) %></p>
    <p>Age: <%= user.age %></p>
    <p>About: <%= user.dedication %></p>
    <h2>Location: <%= user.location_address %></h2>
    <%= link_to "see other events", locals_events_path %>

  </figure>
  <% end %>
</div>
